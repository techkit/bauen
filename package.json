{
    "name": "@techkit/bauen",
    "version": "0.0.0",
    "license": "MIT",
    "description": "A javascript build system for libraries",
    "repository": "git@github.com:techkit/bauen.git",
    "contributors": [
        {
            "name": "Renzo Rosas",
            "url": "https://github.com/rrosas1198"
        }
    ],
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "bin": {
        "build": "./dist/cli.mjs"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "jiti ./src/cli",
        "release": "release-it"
    },
    "gitHooks": {
        "commit-msg": "commitlint -e -V"
    },
    "dependencies": {
        "jiti": "^1.12.15",
        "pathe": "^0.2.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.1.0",
        "@release-it/conventional-changelog": "^4.1.0",
        "@techkit/linter-config": "^0.3.1",
        "eslint": "^8.8.0",
        "prettier": "^2.5.1",
        "release-it": "^14.12.4",
        "typescript": "^4.5.5",
        "yorkie": "^2.0.0"
    }
}
